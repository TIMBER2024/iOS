t i = 0; i < n.length; i++) {\n\t\t\t/// #assert n[i] > 2;\n\t\t\tsum += n[i];\n\t\t}\n\t}\n}\n\ncontract SimpleAsserts {\n    // Call me with 1,2,3,4\n    function main(uint a1, uint a2, uint a3, bool b1, uint a4, uint a5, uint a6, uint a7, uint a8) public {\n        /// #assert a1 == 1;\n        uint x = 2;\n        \n        /// #assert a2 == x;\n\tx++;\n        \n        /// #assert a3 == x;\n        if(b1)\n        /** #assert a4 == x + 1; */\n        {\n            /// #assert a5 == x + 2;\n            x++;\n        } else {\n            x++;\n            /// #assert a6 == x + 2;\n            int t;\n        }\n        \n        \n        while (a6 > 0) {\n            /** #assert a7 >= a6+1; */\n            a6 --;\n        }\n\n\t/** #assert a8 == x + 4; */\n\t{\n\t\tx++;\n\t}\n    }\n}\n"}},"compilerVersion":"0.8.10"}